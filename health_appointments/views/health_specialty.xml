<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <data>

        <record id="health_specialty_tree_view" model="ir.ui.view">
            <field name="model">health.specialty</field>
            <field name="arch" type="xml">
                <tree string="Health Specialty" editable="bottom">
                    <field name="name"/>
                    <field name="description"/>
                </tree>
            </field>
        </record>

        <record id="action_health_specialty" model="ir.actions.act_window">
            <field name="name">Health Specialty</field>
            <field name="res_model">health.specialty</field>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="health_specialty_tree_view"/>
        </record>

    </data>

</odoo>